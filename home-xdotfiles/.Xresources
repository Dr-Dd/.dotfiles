URxvt.scrollBar: false
URxvt.font: xft:Inconsolata:size=20:style=Bold,xft:DejaVu Sans Mono:size=20

! special
URxvt.foreground:   #586e75
URxvt.background:   #fdf6e3
URxvt.cursorColor:  #586e75

! black
URxvt.color0:       #002b36
URxvt.color8:       #657b83

! red
URxvt.color1:       #dc322f
URxvt.color9:       #dc322f

! green
URxvt.color2:       #859900
URxvt.color10:      #859900

! yellow
URxvt.color3:       #b58900
URxvt.color11:      #b58901

! blue
URxvt.color4:       #268bd2
URxvt.color12:      #268bd2

! magenta
URxvt.color5:       #6c71c4
URxvt.color13:      #6c71c4

! cyan
URxvt.color6:       #2aa198
URxvt.color14:      #2aa198

! white
URxvt.color7:       #93a1a1
URxvt.color15:      #fdf6e3

! xscreensaver settings
xscreensaver.splash: false

!font settings
xscreensaver.Dialog.headingFont: -misc-fixed-medium-r-semicondensed--10-72-100-100-c-0-iso8859-1 
xscreensaver.Dialog.bodyFont: -misc-fixed-medium-r-semicondensed--10-72-100-100-c-0-iso8859-1
xscreensaver.Dialog.labelFont: -misc-fixed-medium-r-semicondensed--10-72-100-100-c-0-iso8859-1
xscreensaver.Dialog.unameFont: -misc-fixed-medium-r-semicondensed--10-72-100-100-c-0-iso8859-1
xscreensaver.Dialog.buttonFont: -misc-fixed-medium-r-semicondensed--10-72-100-100-c-0-iso8859-1
xscreensaver.Dialog.dateFont: -misc-fixed-medium-r-semicondensed--10-72-100-100-c-0-iso8859-1
xscreensaver.passwd.passwdFont: -misc-fixed-medium-r-semicondensed--10-72-100-100-c-0-iso8859-1

!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:         #ffffff
xscreensaver.Dialog.background:         #111111
xscreensaver.Dialog.topShadowColor:     #111111
xscreensaver.Dialog.bottomShadowColor:  #111111
xscreensaver.Dialog.Button.foreground:  #666666
xscreensaver.Dialog.Button.background:  #ffffff

!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #666666
xscreensaver.Dialog.text.background:    #ffffff
xscreensaver.Dialog.shadowThickness:    2

!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #ff0000
xscreensaver.passwd.thermometer.background:  #000000
xscreensaver.passwd.thermometer.width:       8

!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %a %b %d, %Y

!remove new login button
xscreensaver.newLoginCommand:
